{
  "name": "subzero",
  "version": "0.1.0",
  "description": "Automated REST and GraphQL api for your database",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*",
    "introspection/**/*",
    "LICENSE.txt",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest --config jestconfig.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint src",
    "prepare": "npm run build",
    "prepack": "cp ../LICENSE.txt .",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint"
  },
  "keywords": [
    "rest",
    "graphql",
    "postgrest",
    "postgresql",
    "sqlite",
    "clickhouse"
  ],
  "author": "Ruslan Talpa <ruslan.talpa@subzero.cloud>",
  "license": "SEE LICENSE IN LICENSE.txt",
  "dependencies": {
    "subzero-wasm": "file:../subzero-wasm/pkg"
  },
  "bundleDependencies": [
    "subzero-wasm"
  ],
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "eslint": "^8.24.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.3.0",
    "eslint-plugin-promise": "^6.0.1",
    "jest": "^28.1.3",
    "prettier": "^2.7.1",
    "sqlite": "^4.1.2",
    "sqlite3": "^5.1.1",
    "ts-jest": "^28.0.7",
    "typescript": "^4.8.4"
  }
}
