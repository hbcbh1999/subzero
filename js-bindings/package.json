{
  "name": "subzerocloud",
  "version": "3.0.1",
  "description": "Automated REST and GraphQL api for your database",
  "type": "module",
  "scripts": {
    "clean": "rm -rf dist-rest && rm -rf dist-rest-web && rm -rf declarations",
    "build": "npm run esbuild && npm run declarations && npm run rollup",
    "declarations": "tsc --declaration --emitDeclarationOnly --outDir declarations",
    "cpdeclarations": "cp declarations/*.d.ts dist-rest/",
    "esbuild": "node esbuild.config.cjs",
    "rollup": "npx rollup --config",
    "test": "jest --config jestconfig.json --forceExit",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint src",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint"
  },
  "keywords": [
    "rest",
    "graphql",
    "postgrest",
    "postgresql",
    "sqlite",
    "clickhouse",
    "mysql"
  ],
  "author": "Ruslan Talpa <ruslan.talpa@subzero.cloud>",
  "license": "SEE LICENSE.txt",
  "devDependencies": {
    "@libsql/client": "^0.12.0",
    "@sqlite.org/sqlite-wasm": "^3.46.1-build3",
    "@types/better-sqlite3": "^7.6.4",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.1.2",
    "@types/node": "^22.5.5",
    "@types/pg": "^8.10.2",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/sqlite3": "^3.1.11",
    "dotenv": "^16.0.3",
    "esbuild": "^0.23.1",
    "esbuild-plugin-copy": "^2.0.2",
    "esbuild-plugin-replace-regex": "^0.0.2",
    "eslint-config-love": "^66.0.0",
    "eslist": "^1.0.0-beta.1",
    "jest": "^29.1.2",
    "mysql2": "^3.2.0",
    "next": "^14.2.12",
    "node-mocks-http": "^1.11.0",
    "pg": "^8.10.0",
    "prettier": "^3.3.3",
    "rollup": "^4.21.3",
    "rollup-plugin-dts": "^6.1.1",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.2",
    "ts-jest": "^29.0.3",
    "ts-jest-mock-import-meta": "^1.2.0",
    "ts-loader": "^9.4.1",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-promise": "^7.1.0"
  }
}
