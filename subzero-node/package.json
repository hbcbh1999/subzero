{
  "name": "subzerocloud",
  "version": "0.1.16",
  "description": "Automated REST and GraphQL api for your database",
  "type": "module",
  "scripts": {
    "build": "npm run esbuild && npm run declarations && npm run rollup",
    "declarations": "tsc --declaration --emitDeclarationOnly --outDir declarations",
    "cpdeclarations": "cp declarations/*.d.ts dist-rest/",
    "esbuild": "node esbuild.config.cjs",
    "rollup": "npx rollup --config",
    "test": "jest --config jestconfig.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint src",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint"
  },
  "keywords": [
    "rest",
    "graphql",
    "postgrest",
    "postgresql",
    "sqlite",
    "clickhouse",
    "mysql"
  ],
  "author": "Ruslan Talpa <ruslan.talpa@subzero.cloud>",
  "license": "SEE LICENSE.txt",
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/jest": "^29.1.2",
    "@types/node": "^18.13.0",
    "@types/pg": "^8.6.6",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "dotenv": "^16.0.3",
    "esbuild": "^0.15.10",
    "esbuild-plugin-copy": "^2.0.2",
    "esbuild-plugin-replace-regex": "^0.0.2",
    "eslint": "^8.25.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.3.0",
    "eslint-plugin-promise": "^6.0.1",
    "jest": "^29.1.2",
    "mysql2": "^3.2.0",
    "next": "^13.4.2",
    "node-mocks-http": "^1.11.0",
    "pg": "^8.10.0",
    "prettier": "^2.7.1",
    "rollup": "^3.2.3",
    "rollup-plugin-dts": "^5.0.0",
    "sqlite": "^4.1.2",
    "sqlite3": "^5.1.2",
    "ts-jest": "^29.0.3",
    "ts-jest-mock-import-meta": "^0.12.0",
    "ts-loader": "^9.4.1",
    "typescript": "^4.8.4"
  }
}
